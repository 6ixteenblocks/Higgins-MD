.cSDcQt [data-component="icon"] { color: var(--fgColor-muted,var(--fgColor-muted,var(--color-fg-muted,#656d76))); -webkit-box-align: center; align-items: center; display: inline-flex; margin-inline-end: var(--control-medium-gap,8px); }

.cSDcQt [data-component="counter"] { margin-inline-start: var(--control-medium-gap,8px); display: flex; -webkit-box-align: center; align-items: center; }

